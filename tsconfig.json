{
    "compilerOptions": {
        "target": "esnext",
        "lib": ["esnext", "dom"],
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "isolatedModules": true,
        "noEmit": true,
        "skipLibCheck": true
    },
    "include": [
        "./src/**/*.ts",
        "./src/**/*.tsx",
        "./*.ts"
    ]
}
